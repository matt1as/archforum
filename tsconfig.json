{
    "compilerOptions": {
      "target": "ES2022",
      "module": "NodeNext",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "skipLibCheck": true,
      "outDir": "./gen/srv/srv",
      "noErrorTruncation": true,
      "moduleResolution": "nodenext",
      "paths": {
        "#cds-models/*": ["./@cds-models/*/index.ts"]
      }
    },
    "include": ["srv"],
    "exclude": ["node_modules"]
  }